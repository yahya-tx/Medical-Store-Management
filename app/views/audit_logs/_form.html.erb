<h1>New Audit Log</h1>

<%= form_with(model: [@branch, @audit_log], local: true) do |form| %>
  <div class="form-group">
    <%= form.label :description %>
    <%= form.text_area :description, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= form.submit 'Create Audit Log', class: 'btn btn-primary' %>
  </div>
<% end %>

<%= link_to 'Back', branch_audit_logs_path(@branch), class: 'btn btn-secondary' %>
